# Example Environment Variables

# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Neon API Key (required)
# Get this from: https://console.neon.tech/app/settings/api-keys
NEON_API_KEY=your_neon_api_key_here

# Google Drive Service Account Credentials (required)
# This should be the entire JSON content from your service account key file
GOOGLE_DRIVE_CREDENTIALS={"type": "service_account", "project_id": "your-project", "private_key_id": "...", "private_key": "...", "client_email": "...", "client_id": "...", "auth_uri": "...", "token_uri": "...", "auth_provider_x509_cert_url": "...", "client_x509_cert_url": "..."}

# Backup Configuration (optional)
BACKUP_RETENTION_DAYS=7
OUTPUT_DIR=./backups
CLEANUP_OLD_BACKUPS=true
CLEANUP_RETENTION_DAYS=30
